function fib () {
  let prev1 = 0
  let prev2 = 1; 
  let current; 
  

    for(let i = 0; i< 8;i++){
      
      if(i ===0) {
        prev1 = 0 
        console.log(0)
      }else if( i===1){
        prev2 = 1
        console.log(1)
      }else {
        current = prev1+prev2
        console.log(current)
        prev1 = prev2; 
        prev2 = current
      }
    }
}


fib(
